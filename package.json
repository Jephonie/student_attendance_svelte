{
	"name": "attendance",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite --host & sleep 7 && chromium --no-sandbox --kiosk --use-gl=angle --use-angle=gl --disable-gpu --disable-software-rasterizer http://localhost:4173",
		"build": "vite build",
		"preview": "vite preview --host & sleep 10 && chromium --no-sandbox --kiosk --use-gl=angle --use-angle=gl --disable-gpu --disable-software-rasterizer --noerrdialogs --disable-infobars --start-fullscreen --incognito http://localhost:4173",
		"prepare": "svelte-kit sync || echo ''",
		"format": "prettier --write .",
		"lint": "prettier --check ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.1.0",
		"@sveltejs/kit": "^2.43.7",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"svelte": "^5.0.0",
		"vite": "^7.0.4"
	},
	"dependencies": {
		"body-parser": "^2.2.0",
		"canvas": "^3.2.0",
		"express": "^5.1.0",
		"face-api.js": "^0.22.2",
		"html5-qrcode": "^2.3.8",
		"jsqr": "^1.4.0"
	},
	"overrides": {
		"cookie": ">=0.7.0",
		"node-fetch": ">=2.6.7"
	}
}
